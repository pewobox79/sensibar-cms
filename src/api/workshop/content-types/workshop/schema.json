{
  "kind": "collectionType",
  "collectionName": "workshops",
  "info": {
    "singularName": "workshop",
    "pluralName": "workshops",
    "displayName": "workshop",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "format": {
      "type": "enumeration",
      "enum": [
        "Workshop",
        "Vortrag"
      ]
    },
    "title": {
      "type": "string"
    },
    "workshop_date": {
      "type": "date"
    },
    "workshopTimeStart": {
      "type": "time"
    },
    "workshopTimeEnd": {
      "type": "time"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "link": {
      "type": "component",
      "component": "elements.link",
      "repeatable": false
    },
    "description": {
      "type": "blocks"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "online",
        "hybrid",
        "inPerson"
      ]
    },
    "uid": {
      "type": "uid",
      "targetField": "title"
    },
    "workshop_registration": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::workshop-registration.workshop-registration",
      "inversedBy": "workshops"
    },
    "contacts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::contact.contact",
      "mappedBy": "workshops"
    },
    "location": {
      "type": "component",
      "component": "customer-data.address-data",
      "repeatable": false
    },
    "ws_status": {
      "type": "enumeration",
      "enum": [
        "planned",
        "cancelled",
        "confirmed"
      ]
    },
    "seoData": {
      "type": "component",
      "component": "elements.seo",
      "repeatable": false,
      "required": false
    },
    "event_tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-ticket.event-ticket",
      "mappedBy": "workshop"
    }
  }
}
